{
	"name": "@fstwon/styles",
	"version": "1.0.0",
	"description": "fstwon styles set",
	"main": "tailwind/index.css",
	"style": "tailwind/index.css",
	"exports": {
		".": "./tailwind/index.css",
		"./scss/*": "./scss/*",
